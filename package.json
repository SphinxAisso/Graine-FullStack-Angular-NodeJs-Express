{
  "name": "app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "backend:devold": "./node_modules/.bin/nodemon -- ./node_modules/.bin/babel-node src/server/server.js",
    "backend:dev": "./node_modules/.bin/babel-node src/server/server.js",
    "backend:build": "./node_modules/.bin/babel src/server --source-maps --out-dir dist",
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "dist": "node ./dist/server",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack --progress --profile --bail && npm run backend:build",
    "server": "webpack-dev-server --inline --progress --port 8001",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tslint --force \"src/client/**/*.ts\"",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "pretest": "npm run lint",
    "test": "karma start",
    "posttest": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json src/client/app/app.component.ts",
    "start": "npm run server",
    "start:hmr": "npm run server -- --hot",
    "postinstall": "npm run webdriver-update"
  },
  "dependencies": {
    "@angular/animations": "4.0.0",
    "@angular/common": "4.0.0",
    "@angular/compiler": "4.0.0",
    "@angular/core": "4.0.0",
    "@angular/forms": "4.0.0",
    "@angular/http": "4.0.0",
    "@angular/platform-browser": "4.0.0",
    "@angular/platform-browser-dynamic": "4.0.0",
    "@angular/router": "4.0.0",
    "angular2-prettyjson": "^2.0.1",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "core-js": "2.4.1",
    "debug": "^2.5.2",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "express-validation": "^1.0.1",
    "joi": "^10.1.0",
    "json-editor": "^0.7.28",
    "jsonwebtoken": "^7.2.1",
    "lodash": "4.17.4",
    "moment": "2.17.1",
    "morgan": "^1.7.0",
    "normalize.css": "3.0.3",
    "nedb": "^1.8.0",
    "reflect-metadata": "0.1.8",
